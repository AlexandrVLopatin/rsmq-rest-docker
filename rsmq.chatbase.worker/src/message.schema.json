{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "user_id",
    "platform",
    "type",
    "intent",
    "message"
  ],
  "properties": {
    "user_id": {
      "$id": "#/properties/user_id",
      "type": "string",
      "title": "The User_id Schema"
    },
    "platform": {
      "$id": "#/properties/platform",
      "type": "string",
      "title": "The Platform Schema"
    },
    "type": {
      "$id": "#/properties/type",
      "type": "string",
      "enum": [
        "agent",
        "user"
      ],
      "title": "The Type Schema"
    },
    "intent": {
      "$id": "#/properties/intent",
      "type": "string",
      "title": "The Intent Schema"
    },
    "not_handled": {
      "$id": "#/properties/not_handled",
      "type": "boolean",
      "title": "The Not_handled Schema",
      "default": false
    },
    "message": {
      "$id": "#/properties/message",
      "type": "string",
      "title": "The Message Schema"
    }
  }
}
